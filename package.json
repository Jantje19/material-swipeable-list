{
	"name": "material-swipeable-list",
	"version": "0.3.0",
	"description": "Swipeable Material-UI list component for React",
	"author": "Jantje19",
	"license": "MIT",
	"repository": "Jantje19/material-swipeable-list",
	"source": "src/index.js",
	"type": "module",
	"main": "./dist/index.umd.cjs",
	"module": "./dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.umd.cjs"
		}
	},
	"scripts": {
		"format": "prettier -w ./src/**/*",
		"build": "tsc --noEmit && vite build && tsc --emitDeclarationOnly",
		"predeploy": "cd example && pnpm install && pnpm run build",
		"deploy": "gh-pages -d example/build"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@mui/material": "^5.14.3",
		"@types/react": "^18.2.18",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"@vitejs/plugin-react-swc": "^3.3.2",
		"eslint": "^8.46.0",
		"eslint-plugin-react": "^7.33.1",
		"gh-pages": "^5.0.0",
		"prettier": "^3.0.0",
		"react": "^18.2.0",
		"typescript": "^5.1.6",
		"vite": "^4.4.8"
	},
	"files": [
		"dist"
	]
}
